{
  "name": "webhook-service",
  "version": "1.0.0",
  "description": "A Fastify-based webhook gateway designed to securely receive, validate, and process incoming webhook events. It supports signature validation through configurable secret keys and integrates seamlessly with Pub/Sub systems to publish validated data",
  "main": "server.mjs",
  "engines": {
    "node": ">=17.9.0",
    "npm": ">=8.11.0"
  },
  "scripts": {
    "lint": "eslint . --fix",
    "ci": "eslint . && c8 mocha",
    "semantic-release": "semantic-release",
    "test": "c8 mocha",
    "gitHooks": "chmod +x ./.scripts/hooks/pre-commit && git config core.hooksPath ./.scripts/hooks/"
  },
  "author": "Carl Smith",
  "license": "UNLICENSED",
  "dependencies": {
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.0.0",
    "@google-cloud/pubsub": "^4.7.2",
    "ajv": "^8.17.1",
    "async": "^3.2.5",
    "axios": "^1.7.2",
    "fastify": "^4.28.1",
    "fastify-metrics": "^11.0.0",
    "postgres": "^3.4.4",
    "redis": "^4.6.15",
    "svix": "^1.35.0",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.8",
    "@babel/plugin-syntax-import-assertions": "^7.24.7",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "c8": "^10.1.2",
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.2",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^39.9.1",
    "esm": "^3.2.25",
    "mocha": "^10.6.0",
    "nodemon": "^3.1.5",
    "proxyquire": "^2.1.3",
    "semantic-release": "^19.0.5",
    "sinon": "^18.0.0"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-syntax-import-assertions"
    ]
  }
}
